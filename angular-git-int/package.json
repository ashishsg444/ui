{
  "name": "angular-git-int",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:angular-git-int": "node dist/angular-git-int/server/server.mjs",
    "build:tokens": "node ./scripts/style-dictionary.config.js",
    "deploy": "ng build --prerender=false --ssr=false --output-path dist/angular-git-int --base-href '/angular-git-int/' && npx angular-cli-ghpages --dir=dist/angular-git-int",
    "deploy-option": "ng build --output-path dist --base-href '/angular-git-init/' && npx angular-cli-ghpages --dir=dist/angular-git-init",
    "deploy-ui": "ng build --configuration production --base-href '/ui/' && npx angular-cli-ghpages --dir=dist/ui/browser"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.4",
    "@angular/common": "^20.3.4",
    "@angular/compiler": "^20.3.4",
    "@angular/core": "^20.3.4",
    "@angular/forms": "^20.3.4",
    "@angular/platform-browser": "^20.3.4",
    "@angular/platform-browser-dynamic": "^20.3.4",
    "@angular/platform-server": "^20.3.4",
    "@angular/router": "^20.3.4",
    "@angular/ssr": "^20.3.5",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.5",
    "@angular/build": "^20.3.5",
    "@angular/cli": "^20.3.5",
    "@angular/compiler-cli": "^20.3.4",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "angular-cli-ghpages": "^2.0.3",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "style-dictionary": "^5.1.1",
    "typescript": "~5.9.3"
  }
}
